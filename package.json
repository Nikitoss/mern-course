  "license": "ISC",
  "dependencies": {
    "bcryptjs": "2.4.3",
    "config": "3.3.6",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "express-validator": "6.10.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.12.3",
    "shortid": "2.2.16"
  },
  "devDependencies": {
    "@types/node": "14.14.40",
    "concurrently": "6.0.2",
    "nodemon": "2.0.7"
  }
}
